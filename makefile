
#-------------------------------------------------------------------------------
# make commands
#-------------------------------------------------------------------------------

practica.exe : source/main.c include/linked_list.c
	gcc -Wall source/main.c include/linked_list.h include/linked_list.c -o practica.exe

#-------------------------------------------------------------------------------
# clean commands
#-------------------------------------------------------------------------------
clean : 
	@rm -fv build/main.o
	@rm -fv build/linked_list.o
	@rm -fv practica.exe

#-------------------------------------------------------------------------------
# run command
#-------------------------------------------------------------------------------
run : practica.exe
	./practica.exe

